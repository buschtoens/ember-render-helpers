{
  "name": "ember-codemod-ember-render-helpers-to-v1",
  "version": "1.0.3",
  "description": "Rename helpers from ember-render-helpers",
  "keywords": [
    "codemod",
    "ember-codemod",
    "emberjs"
  ],
  "repository": "buschtoens/ember-render-helpers",
  "license": "MIT",
  "author": "Isaac J. Lee",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/ember-codemod-ember-render-helpers-to-v1.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./build.sh --production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "pnpm build",
    "test": "./build.sh --test && mt dist-for-testing --quiet"
  },
  "dependencies": {
    "@codemod-utils/ast-template": "^1.1.8",
    "@codemod-utils/files": "^2.0.8",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@codemod-utils/tests": "^1.1.11",
    "@sondr3/minitest": "^0.1.2",
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^18.19.76",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": "18.* || >= 20"
  }
}
