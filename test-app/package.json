{
  "name": "test-app",
  "version": "1.0.3",
  "private": true,
  "description": "Test app for ember-render-helpers",
  "keywords": [
    "ember-helper",
    "ember-template-helper",
    "template-helper",
    "helper",
    "render",
    "@ember/render-modifiers",
    "did-insert",
    "did-update",
    "will-destroy"
  ],
  "repository": "buschtoens/ember-render-helpers",
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan@buschtoens.me> (https://jan.buschtoens.me)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "start": "ember serve",
    "test": "ember test",
    "test:ember-compatibility": "./node_modules/.bin/ember try:one"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.1.0",
    "@embroider/test-setup": "^4.0.0",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.5.2",
    "@ijlee2-frontend-configs/ember-template-lint": "^0.4.1",
    "@ijlee2-frontend-configs/eslint-config-ember": "^0.2.2",
    "@ijlee2-frontend-configs/prettier": "^0.2.1",
    "@ijlee2-frontend-configs/typescript": "^0.3.1",
    "@types/qunit": "^2.19.12",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.2.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-qunit": "^9.0.1",
    "ember-render-helpers": "workspace:*",
    "ember-resolver": "^13.1.0",
    "ember-source": "~6.2.0",
    "ember-template-lint": "^6.1.0",
    "ember-try": "^3.0.0",
    "eslint": "^9.20.1",
    "latest-version": "^9.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.1",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0"
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "ember": {
    "edition": "octane"
  }
}
